@model IEnumerable<SosCidadaoWeb.Models.TipopertenceModel>

@{
    ViewData["Title"] = "Tipo Pertence";
    Layout = "_LayoutDashboard";
}

<h1> </h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoPertence)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdOrganizacao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoPertence)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdOrganizacao)
            </td>
            <td>    
                @Html.ActionLink("Editar", "Edit", new { id=item.IdTipoPertence }) |
                @Html.ActionLink("Detalhes", "Details", new { id = item.IdTipoPertence }) |
                @Html.ActionLink("Remover", "Delete", new { id = item.IdTipoPertence })
            </td>
        </tr>
}
    </tbody>
</table>
