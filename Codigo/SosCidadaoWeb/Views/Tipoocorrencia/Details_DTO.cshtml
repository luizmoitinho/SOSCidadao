@model Core.DTO.TipoocorrenciaDTO

@{
    ViewData["Title"] = "Details_DTO";
    Layout = "_LayoutDashboard";
}

<div>
    <dl class="row">
        <div class="col-sm-2">
           Nº Cadastral
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdTipoOcorrencia)
        </dd>
        <div class="col-sm-2">
           Nome
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <div class="col-sm-2">
            Organização
        </div>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeFantasiaOrganizacao)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.IdTipoOcorrencia }) |
    <a asp-action="Index">Voltar</a>
</div>
